{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "cross-env NODE_ENV=production npm-run-all -p build:*",
    "dev": "cross-env NODE_ENV=development concurrently \"npm run dev:*\" \"remix dev\"",
    "build:remix": "remix build",
    "build:css": "tailwindcss -i ./styles/tailwind.css -o ./app/tailwind.css --minify",
    "build:worker": "esbuild ./app/entry.worker.ts --outfile=./public/entry.worker.js --minify --bundle --format=esm --define:process.env.NODE_ENV='\"production\"'",
    "dev:remix": "remix dev",
    "dev:css": "tailwindcss -i ./styles/tailwind.css -o ./app/tailwind.css --watch",
    "dev:worker": "esbuild ./app/entry.worker.ts --outfile=./public/entry.worker.js --bundle --format=esm --define:process.env.NODE_ENV='\"development\"' --watch"
  },
  "dependencies": {
    "@remix-run/node": "^1.6.3",
    "@remix-run/react": "^1.6.3",
    "@remix-run/vercel": "^1.6.3",
    "@vercel/node": "^1.15.2",
    "axios": "^0.27.2",
    "cross-env": "^7.0.3",
    "dayjs": "^1.11.3",
    "dotenv": "^16.0.0",
    "node-persist": "^3.1.0",
    "npm-run-all": "^4.1.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "web-push": "^3.4.5",
    "zod": "^3.17.3"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.6.3",
    "@remix-run/eslint-config": "^1.6.3",
    "@remix-run/serve": "^1.6.3",
    "@types/node-persist": "^3.1.2",
    "@types/react": "^17.0.45",
    "@types/react-dom": "^17.0.17",
    "autoprefixer": "^10.4.7",
    "concurrently": "^7.2.2",
    "eslint": "^8.15.0",
    "postcss": "^8.4.14",
    "tailwindcss": "^3.1.4",
    "typescript": "^4.6.4"
  },
  "engines": {
    "node": ">=14"
  }
}
